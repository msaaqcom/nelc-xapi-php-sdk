{
    "name": "msaaq/nelc-xapi-sdk",
    "description": "xAPI Integration with Saudi NELC (National Center for e-Learning)",
    "keywords": [
        "msaaqcom",
        "nelc-xapi-sdk",
        "xapi-statements"
    ],
    "homepage": "https://github.com/msaaqcom/nelc-xapi-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Hussam Abd",
            "email": "hussam@msaaq.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Msaaq\\Nelc\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
